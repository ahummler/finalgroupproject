<%= render 'form' %>
<div id="card">
  <div class="front">
     <p id="company_name"><%= @card.company_name %></p>
     <p id="full_name"><%= @card.full_name %></p>
     <p id="job_title"><%= @card.job_title %></p>
     <p id="phone"><%= @card.phone %></p>
     <p id="email"><%= @card.email %></p>
  </div>
  <div class="back">
    <div class="pic">
     <p><%= image_tag @card.avatar.url(:medium) %></p>
    </div>
     <p id="web_url"><%= @card.web_url%></p>
  </div>
</div>