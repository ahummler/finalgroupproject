<%= form_for @card, html: { multipart: true } do |f| %> 
	<p>
		<%= f.label  :company_name, "NAME" %>
		<%= f.text_field :company_name, placeholder: "Company Name" %>
	</p>
	<p>
		<%= f.label :web_url, "TITLE" %>
		<%= f.text_field :web_url, placeholder: "Web URL" %>
	</p>
	<p>
		<%= f.label :full_name, "EMAIL" %>
		<%= f.text_field :full_name,  placeholder: "Full Name" %>
	</p>
	<p>
		<%= f.label :job_title, "PHONE" %>
		<%= f.text_field :job_title, placeholder: "Job Title" %>
	<p>
		<%= f.submit %>
	</p>
<% end %>